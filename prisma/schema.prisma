generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id String @id @default(cuid()) @unique
  name String?
  email String @unique
}

model Room {
  id String @id @default(cuid()) @unique
  expiredAt String
  connected String[]
}